<Window x:Class="Sequencer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sequencer"
        mc:Ignorable="d"
        Title="Sequencer" Height="500" Width="1200">
    <Grid>
        <Canvas x:Name="SequencerCanvas" MouseMove="SequencerMouseMoved" MouseRightButtonDown="SequencerMouseDown"
                MouseRightButtonUp="SequencerMouseUp" />
    </Grid>
</Window>